symbols_of_great_power =
  * '@'
  * '#'
  * '!'
  * '$'
  * '%'
  * '&'
  * '*'

string[k] = v for k, v in pairs {
  stutter: (text, types={'upper'}, chance=5) ->
    text_tab = string.split text, ""
    effects =
      upper: (text) -> string.upper text
      scramble: (text) -> symbols_of_great_power[ğŸ²(#symbols_of_great_power)]
    new_text = ""
    for t in *text_tab
      if ğŸ²(1, 100) <= chance
        key = types[ğŸ²(#types)]
        t = effects[key] t

      new_text ..= t

    return new_text
}
